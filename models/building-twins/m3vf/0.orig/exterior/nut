FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    back
    {
        type            empty;
    }

    front
    {
        type            empty;
    }

    maxX
    {
        type            calculated;
        value           uniform 0;
    }

    maxZ
    {
        type            calculated;
        value           uniform 0;
    }

    minX
    {
        type            calculated;
        value           uniform 0;
    }

    exterior_to_ground
    {
        type            nutkWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0;
    }

    exterior_to_envelope
    {
        type            nutkWallFunction;
        Cmu             0.09;
        kappa           0.41;
        E               9.8;
        value           uniform 0;
    }
}


